<h1>CREATE CEREAL</h1>

<%= form_for @cereal do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :brand %>
  <%= f.text_field :brand %>

  <%= f.label :release_year %>
  <%= f.number_field :release_year %>

  <%= f.label :price %>
  <%= f.number_field :price %>

  <%= f.label :img_url %>
  <%= f.text_field :img_url %>

  <%= f.submit %>
<% end %>